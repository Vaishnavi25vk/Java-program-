class Temperature
   {
   static double fahrenheitToCelsius(double fahrenheit) 
    {
        return (fahrenheit - 32) * 5 / 9;
    }
    static double celsiusToFahrenheit(double celsius)
    {
        return (celsius * 9 / 5) + 32;
    }
    public static void main(String[] args) {
        double fahrenheit = 98.6;
        double celsius = 37;
        double celsiusResult = fahrenheitToCelsius(fahrenheit);
        System.out.println(fahrenheit + " Fahrenheit is " + celsiusResult + " Celsius.");
        double fahrenheitResult = celsiusToFahrenheit(celsius);
        System.out.println(celsius + " Celsius is " + fahrenheitResult + " Fahrenheit.") ;
    }
}
